let yellow = #ffcd00 in
let blue = #004b87 in
let red = #da291c in
let white = #ffffff in

let number = 2 + 3 - 4 * 5 / 6 in
let more_than_one = number > 1 in
let less_than_two = number < 2 in
let between_one_and_two = more_than_one and less_than_two in
let true_or_false = true or true and false xor false in
let negative = not -5 > 0 in

if between_one_and_two then
    if true_or_false then
        blue
    else
        yellow
else if negative then
    white
else
    red
